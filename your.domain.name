server {
    server_name server_name;
    location / {
        proxy_pass http://127.0.0.1:3000; # tu ip de n8n expuesta desde docker
        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
    }

    #  Certificado ssl por Certbot (generalo de forma autom√°tica)

    listen 443 ssl http2; # managed by Certbot
    ssl_certificate 
    ssl_certificate_key 
    include 
    ssl_dhparam 

}